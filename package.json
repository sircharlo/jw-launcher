{
  "name": "jw-launcher",
  "version": "0.1.5",
  "description": "JW Launcher",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sircharlo/jw-launcher.git"
  },
  "dependencies": {
    "@electron/remote": "^1.2.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@popperjs/core": "^2.9.2",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.2",
    "bootstrap-dark-5": "^1.0.2",
    "electron-updater": "^4.3.10",
    "graceful-fs": "^4.2.6",
    "is-port-reachable": "^3.0.0",
    "jquery": "^3.6.0",
    "loudness": "^0.4.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "power-control": "^1.0.0",
    "select2": "^4.1.0-rc.0",
    "urllib": "^2.37.3"
  },
  "build": {
    "appId": "sircharlo.jw-launcher",
    "productName": "JW Launcher",
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "category": "Utilities",
      "target": "AppImage",
      "icon": "build/icon.png",
      "publish": [
        "github"
      ]
    },
    "buildDependenciesFromSource": true
  },
  "devDependencies": {
    "electron": "^13.2.2",
    "electron-builder": "^22.11.7",
    "eslint": "^7.31.0"
  },
  "scripts": {
    "start": "electron .",
    "pack-win": "electron-builder --win",
    "pack-win-test": "electron-builder --dir --win",
    "pack-linux-test": "electron-builder --dir --linux",
    "pack-linux": "electron-builder --linux",
    "publish": "electron-builder --win -p always",
    "publish-linux": "electron-builder --linux -p always",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder"
  },
  "author": "",
  "license": "ISC"
}
