{
  "name": "jw-launcher",
  "version": "25.10.0",
  "description": "JW Launcher",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sircharlo/jw-launcher.git"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "bootstrap-dark-5": "^1.1.3",
    "electron-updater": "^6.6.2",
    "graceful-fs": "^4.2.11",
    "inputmask": "^5.0.9",
    "jquery": "^3.7.1",
    "path": "^0.12.7",
    "power-control": "^1.0.0",
    "select2": "^4.1.0-rc.0"
  },
  "build": {
    "appId": "sircharlo.jw-launcher",
    "productName": "JW Launcher",
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "category": "Utilities",
      "target": "AppImage",
      "icon": "build/icon.png",
      "publish": [
        "github"
      ]
    },
    "mac": {
      "target": {
        "target": "dmg",
        "arch": [
          "universal"
        ]
      },
      "publish": [
        "github"
      ]
    },
    "buildDependenciesFromSource": true
  },
  "devDependencies": {
    "electron": "^38.4.0",
    "electron-builder": "^26.0.12"
  },
  "scripts": {
    "start": "electron .",
    "pack-win": "electron-builder --win",
    "pack-win-test": "electron-builder --dir --win",
    "pack-linux-test": "electron-builder --dir --linux",
    "pack-linux": "electron-builder --linux",
    "publish": "electron-builder --win -p always",
    "publish-linux": "electron-builder --linux -p always",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder"
  },
  "author": "",
  "license": "ISC"
}
