{
  "author": "",
  "build": {
    "appId": "sircharlo.jw-launcher",
    "buildDependenciesFromSource": true,
    "linux": {
      "category": "Utilities",
      "icon": "build/icon.png",
      "publish": [
        "github"
      ],
      "target": "AppImage"
    },
    "mac": {
      "publish": [
        "github"
      ],
      "target": {
        "arch": [
          "universal"
        ],
        "target": "dmg"
      }
    },
    "productName": "JW Launcher",
    "win": {
      "icon": "build/icon.ico",
      "publish": [
        "github"
      ],
      "target": "NSIS"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.13.5",
    "bootstrap": "^5.3.8",
    "bootstrap-dark-5": "^1.1.3",
    "electron-updater": "^6.7.3",
    "graceful-fs": "^4.2.11",
    "inputmask": "^5.0.9",
    "jquery": "^4.0.0",
    "path": "^0.12.7",
    "power-control": "^1.0.0",
    "select2": "^4.1.0-rc.0"
  },
  "description": "JW Launcher",
  "devDependencies": {
    "electron": "^40.3.0",
    "electron-builder": "^26.7.0"
  },
  "license": "ISC",
  "main": "main.js",
  "name": "jw-launcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/sircharlo/jw-launcher.git"
  },
  "scripts": {
    "pack-linux": "electron-builder --linux",
    "pack-linux-test": "electron-builder --dir --linux",
    "pack-win": "electron-builder --win",
    "pack-win-test": "electron-builder --dir --win",
    "postinstall": "electron-builder install-app-deps",
    "publish": "electron-builder --win -p always",
    "publish-linux": "electron-builder --linux -p always",
    "release": "electron-builder",
    "start": "electron ."
  },
  "version": "26.1.0"
}
